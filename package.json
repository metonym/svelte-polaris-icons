{
  "name": "svelte-polaris-icons",
  "version": "4.1.0",
  "license": "MIT",
  "description": "Shopify Polaris SVG icons as Svelte components",
  "author": "Eric Liu (https://github.com/metonym)",
  "svelte": "./lib/index.js",
  "main": "./lib/index.js",
  "types": "./types/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "dev": "rollup -cw",
    "predeploy": "node build && rollup -c",
    "deploy": "npx gh-pages -d dist",
    "prepack": "s2s --input=node_modules/@shopify/polaris-icons/dist/svg && npx sveld --glob"
  },
  "devDependencies": {
    "@shopify/polaris-icons": "4.5.0",
    "svelte-readme": "^3.1.0",
    "svg-to-svelte": "^2.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/metonym/svelte-polaris-icons"
  },
  "homepage": "https://github.com/metonym/svelte-polaris-icons",
  "bugs": "https://github.com/metonym/svelte-polaris-icons/issues",
  "keywords": [
    "svelte",
    "svelte component",
    "shopify",
    "shopify polaris",
    "polaris",
    "icons",
    "svg"
  ],
  "files": [
    "lib",
    "types"
  ]
}
